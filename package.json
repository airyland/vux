{
  "name": "vux",
  "description": "vue adapter for weui",
  "author": "airyland <airyland@qq.com>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "rimraf dist && webpack --progress --hide-modules --colors --config build/webpack.prod.conf.js && cd dist/static && mkdir vendors && cd ../../ && cp src/assets/vendors/* dist/static/vendors && cp CNAME dist/",
    "test": "karma start build/karma.conf.js --single-run",
    "deploy": "gh-pages -d dist",
    "xdeploy": "rimraf dist && webpack --progress --hide-modules --colors --config build/webpack.prod.conf.js && gh-pages -d dist"
  },
  "dependencies": {
    "lodash": "^4.3.0",
    "validator": "^3.39.0",
    "vue": "^1.0.16",
    "vue-focus": "^0.1.1",
    "vue-router": "^0.7.11",
    "zepto": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^5.8.0",
    "connect-history-api-fallback": "^1.1.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "function-bind": "^1.0.2",
    "gh-pages": "^0.10.0",
    "html-webpack-plugin": "^1.7.0",
    "inject-loader": "^2.0.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "less-loader": "^2.2.2",
    "phantomjs": "^1.9.19",
