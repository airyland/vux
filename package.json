{
  "name": "vux",
  "version": "0.0.8",
  "description": "Mobile web UI based on Vue and Weui",
  "author": "airyland <airyland@qq.com>",
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "rimraf dist && webpack --progress --hide-modules --colors --config build/webpack.prod.conf.js && cd dist/static && mkdir vendors && cd ../../ && cp -r src/assets/* dist/static/ && cp CNAME dist/ && cp favicon.ico dist/ && node build/fetch-milestone.js && cp build/milestone.json dist/",
    "test": "karma start build/karma.conf.js --single-run",
    "deploy": "gh-pages -d dist",
    "xdeploy": "npm run build && gh-pages -d dist",
    "publish": "webpack --progress --hide-modules --colors --config build/webpack.npm.conf.js"
  },
  "main": "vux.js",
  "dependencies": {
    "blazy": "^1.5.2",
    "countup": "^1.6.1",
    "lodash": "^4.3.0",
    "shake.js": "^1.2.2",
    "validator": "^3.39.0",
    "vue-focus": "^0.1.1",
    "vue-lazyload": "^0.2.9",
    "webp-support": "^1.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^5.8.0",
    "co": "^4.6.0",
    "co-fs": "^1.2.0",
    "co-request": "^1.0.0",
    "connect-history-api-fallback": "^1.1.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "function-bind": "^1.0.2",
    "gh-pages": "^0.10.0",
    "html-webpack-plugin": "^1.7.0",
    "inject-loader": "^2.0.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.15",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "less-loader": "^2.2.2",
    "phantomjs": "^1.9.19",
    "rimraf": "^2.5.0",
    "url-loader": "^0.5.7",
    "vue": "^1.0.17",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.0.0",
    "vue-router": "^0.7.11",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
